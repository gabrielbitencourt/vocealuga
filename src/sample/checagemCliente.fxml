<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Pane prefHeight="30.0" prefWidth="600.0" style="-fx-background-color: black;">
            <children>
                <Button layoutY="1.0" mnemonicParsing="false" onAction="#acessoHome" prefHeight="26.0" style="-fx-background-color: black" text="Home" textFill="WHITE" />
                <Button layoutX="291.0" layoutY="1.0" mnemonicParsing="false" onAction="#acessoChecagemCliente" prefHeight="26.0" style="-fx-background-color: black;" text="Cliente" textFill="WHITE" />
                <Button layoutX="444.0" layoutY="1.0" mnemonicParsing="false" prefHeight="26.0" style="-fx-background-color: black;" text="Lista Negra" textFill="WHITE" />
                <Button layoutX="556.0" layoutY="1.0" mnemonicParsing="false" onAction="#acessoSample" prefHeight="26.0" style="-fx-background-color: Black;" text="Sair" textFill="WHITE" />
                <Button layoutX="370.0" layoutY="1.0" mnemonicParsing="false" onAction="#acessoCenaCadastroVeiculo" prefHeight="26.0" style="-fx-background-color: black;" text="Veiculo" textFill="WHITE" />
            </children>
        </Pane>
      <TextField layoutX="215.0" layoutY="116.0" promptText="CPF" />
      <Button layoutX="272.0" layoutY="157.0" mnemonicParsing="false" onAction="#acessoHome" text="Entrar" />
      <Label layoutX="170.0" layoutY="80.0" text="CPF do cliente ja cadastrado:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="243.0" layoutY="261.0" mnemonicParsing="false" onAction="#acessoCenaCadastroUsuario" text="Fazer Cadastro" />
    </children>
</AnchorPane>
