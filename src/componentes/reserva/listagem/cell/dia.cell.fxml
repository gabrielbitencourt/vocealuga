<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="100.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="componentes.reserva.listagem.cell.DiaCell">
   <top>
      <Label fx:id="diaLabel" text="Dia" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="3.0" />
         </BorderPane.margin></Label>
   </top>
   <center>
      <AnchorPane>
         <Button layoutX="20.0" layoutY="3.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="60.0" text="x retiradas" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" BorderPane.alignment="CENTER">
            <font>
               <Font size="10.0" />
            </font>
         </Button>
         <Button layoutX="20.0" layoutY="27.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="60.0" text="x entregas" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" BorderPane.alignment="CENTER">
            <font>
               <Font size="10.0" />
            </font>
         </Button>
      </AnchorPane>
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#novaReserva" text="+" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="3.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
